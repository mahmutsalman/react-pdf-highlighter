{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "dialog:default",
    "fs:default",
    "fs:allow-read-file",
    "fs:allow-document-read-recursive",
    "fs:allow-home-read-recursive",
    "sql:allow-load",
    "sql:allow-execute",
    "sql:allow-select"
  ]
}
